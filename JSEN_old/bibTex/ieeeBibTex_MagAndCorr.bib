@article{alonso2013,
	title = {Double helix dipole design applied to magnetic resonance: A novel {NMR} coil},
	volume = {235},
	issn = {1090-7807},
	url = {http://www.sciencedirect.com/science/article/pii/S1090780713001730},
	doi = {10.1016/j.jmr.2013.07.004},
	shorttitle = {Double helix dipole design applied to magnetic resonance},
	abstract = {A new radio frequency coil design for {NMR} experiments is presented. The coil generates a magnetic field purely perpendicular to the longitudinal axis of the main magnet, and its sensitivity is higher than the traditional transversal resonators. This is achieved by adding the contribution of two tilted solenoid coils fed with opposite currents. The work presents the mathematical model for the new coil, numerical simulations performed to validate that model and a comparison with an equivalent saddle coil. The new design is tested experimentally in low- and high-field {NMR} experiments and compared with results obtained with equivalent saddle coils. The results lead to conclude that the new design provides better sensitivity than the transverse resonators commonly used in {NMR}.},
	pages = {32--41},
	journaltitle = {Journal of Magnetic Resonance},
	author = {Alonso, J. and Soleilhavoup, A. and Wong, A. and Guiga, A. and Sakellariou, D.},
	urldate = {2018-02-05},
	date = {2013-10-01},
	keywords = {Double helix dipole, {NMR} detection, {NMR} sensitivity, Tilted coil},
	file = {Double-helix-dipole-design-applied-to-magnetic-reso_2013_Journal-of-Magnetic.pdf:C\:\\Users\\Javier Alonso\\Zotero\\storage\\NZSVHRPL\\Double-helix-dipole-design-applied-to-magnetic-reso_2013_Journal-of-Magnetic.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Javier Alonso\\Zotero\\storage\\BDN78DPT\\S1090780713001730.html:text/html}
}

@article{damadian1971,
	title = {Tumor Detection by Nuclear Magnetic Resonance},
	volume = {171},
	rights = {© 1971},
	issn = {0036-8075, 1095-9203},
	url = {http://science.sciencemag.org/content/171/3976/1151},
	doi = {10.1126/science.171.3976.1151},
	abstract = {Spin echo nuclear magnetic resonance measurements may be used as a method for discriminating between malignant tumors and normal tissue. Measurements of spin-lattice (T1) and spin-spin (T2) magnetic relaxation times were made in six normal tissues in the rat (muscle, kidney, stomach, intestine, brain, and liver) and in two malignant solid tumors, Walker sarcoma and Novikoff hepatoma. Relaxation times for the two malignant tumors were distinctly outside the range of values for the normal tissues studied, an indication that the malignant tissues were characterized by an increase in the motional freedom of tissue water molecules. The possibility of using magnetic relaxation methods for rapid discrimination between benign and malignant surgical specimens has also been considered. Spin-lattice relaxation times for two benign fibroadenomas were distinct from those for both malignant tissues and were the same as those of muscle.},
	pages = {1151--1153},
	number = {3976},
	journaltitle = {Science},
	author = {Damadian, Raymond},
	urldate = {2018-02-09},
	date = {1971-03-19},
	langid = {english},
	pmid = {5544870},
	file = {damadian1971.pdf:C\:\\Users\\Javier Alonso\\Zotero\\storage\\4DMVLTB2\\damadian1971.pdf:application/pdf;Snapshot:C\:\\Users\\Javier Alonso\\Zotero\\storage\\KBC9G95N\\1151.html:text/html}
}

