	keywords = {B0-{MAS}, Coherent averaging, Field spinning, High-resolution, Hyperpolarized xenon, Magic angle field rotation, Rotating field},
	file = {ScienceDirect Snapshot:C\:\\Users\\Javier Alonso\\Zotero\\storage\\LIV9YSKR\\S1090780704000886.html:text/html}
}

